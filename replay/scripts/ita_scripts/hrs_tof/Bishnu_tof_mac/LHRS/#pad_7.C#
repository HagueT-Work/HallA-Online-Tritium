 T->Draw(Form("((LTDC.F1FirstHit[37] -LTDC.F1FirstHit[47])*56.23e-12 - (((LTDC.F1FirstHit[30]-LTDC.F1FirstHit[%d])*56.23e-12 +((LTDC.F1FirstHit[37]-LTDC.F1FirstHit[%d])*56.23e-12))/2.0 + 9.09441e-09*L.tr.x[0]  -3.67840e-08*L.tr.th[0] -1.02470e-12*L.s2.la_c[7] ))>>h7",pmt-1,pmt+47),cut_7);




 return 0;
}
